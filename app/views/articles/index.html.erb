  <div class="container">
<div class="row">
            <!-- Blog Entries Column -->
            <div class="col-md-8">

                <h1 class="page-header">Listing Blogs
                </h1>
        <% @articles.each do |article| %>
        <h2><%= article.title %></h2>
        <p><span class="glyphicon glyphicon-time"></span> Published at <%= article.published_at %></p>
        <hr>
        <p>
          <%= article.excerpt %>
        </p>
        <div><%= link_to 'Read more >', article %></div>
        <% end %>
      </div>
</div>
<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th>Excerpt</th>
      <th>Body</th>
      <th>Published at</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @articles.each do |article| %>
      <tr>
        <td><%= article.title %></td>
        <td><%= article.location %></td>
        <td><%= article.excerpt %></td>
        <td><%= article.body %></td>
        <td><%= article.published_at %></td>
        <td><%= link_to 'Show', article %></td>
        <td><%= link_to 'Edit', edit_article_path(article) %></td>
        <td><%= link_to 'Destroy', article, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
</div>

